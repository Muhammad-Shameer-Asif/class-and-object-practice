class complex:
    def __init__(self,real, img):
        self.real=real
        self.img = img

    def shownum(self):
        print(self.real,"i","+",self.img,"j")

    def add(obj1,obj2):
        newreal=obj1.real + obj2.real
        newimg= obj1.img + obj2.img
        return complex(newreal,newimg)


num1=complex(8,6)
num1.shownum()

num2=complex(4,2)
num2.shownum()

num3 = num1.add(num2)
num3.shownum()