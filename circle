class circle:
    def __init__(self , radius):
        self.radius =radius

    def area(self):
        print("The area of circle is :",3.1416*self.radius*self.radius)


    def primeter(self):
        print("The perimeter of circle is :",2*3.1416*self.radius)    



c1 = circle(5)
c1.area()
c1.primeter()
