class car:
    color = "white"
    
    @staticmethod
    def car_start():
        print("car started ..")
        return 

    def car_stop(self):
        print("car stopped ..")


class toyota_car(car):
    def __init__(self , name):
        self.name =name


c1=toyota_car("fortuener")
c2 = toyota_car("land cruiser")


print(c2.car_start())
print(c2.car_stop())
print(c2.color)
print(c2.name) 